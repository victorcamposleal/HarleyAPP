{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parser":"babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension":0,
        "no-multiple-empty-lines":0,
        "keyword-spacing":0,
        "semi":0,
        "object-curly-spacing":0,
        "react/no-did-mount-set-state": 0,
        "no-whitespace-before-property": "off"
    }
}
